import { useContext } from 'react';
import {UserContext} from "../contexts/UserContext";

interface ContainerProps {}

const UserList: React.FC<ContainerProps> = () => {
	let { deleteUser, editUser } = useContext(UserContext);

	const userCreated = (user: any) => {
		editUser({...user, created: true})
			.then(() => {})
			.catch((error: any) => {
				console.log(error);
			});
	};

    const updateUser = (user: any) => {
		editUser({...user, completed: false})
			.then(() => {})
			.catch((error: any) => {
				console.log(error);
			});
	};

	const slideToDelete = (id: number) => {
		deleteUser(id)
			.then(() => {})
			.catch((error: any) => {
				console.log(error);
			});
	};
};

return (
    <div>

);

export default List;
